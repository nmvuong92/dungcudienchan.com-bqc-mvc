        function PurchaseWallet(id) {

            $.ajax({
                url: '@Url.Action("purchasewallet","ajax")?id='+id,
                type: 'GET',
                success: function (response) {
                    $('#myModal_Purchase').find('.modal-body').html(response);
                    $('#myModal_Purchase').modal('show');

                    $('#myModal_Purchase #FeeServiceID').change(function () {
                        var value = $(this).val();
                        if (value > 0) {
                            var el = $('#myModal_Purchase #FeeGateID');
                            var elVal = el.val();
                            el.find('option[value!=""]').remove();
                            $.ajax({
                                url: '@Url.Action("LoadFeeGate", "Ajax")',
                                type: 'POST',
                                data: { id: value },
                                dataType: 'json',
                                cache: false,
                                success: function (data) {
                                    $.each(data, function (index, item) {
                                        el.append($('<option>').text(item.Text).val(item.Value));
                                    });
                                    if (elVal != null) {
                                        el.val(elVal).change();
                                    }
                                }
                            });

                        }
                    }).change();

                    $('#myModal_Purchase #FeeGateID').change(function () {
                        var value = $(this).val();
                        if (value > 0) {
                            var el = $('#myModal_Purchase #FeePriceDetailID');
                            var elVal = el.val();
                            el.find('option[value!=""]').remove();
                            $.ajax({
                                url: '@Url.Action("LoadFeePriceDetail", "Ajax")',
                                type: 'POST',
                                data: { id: value },
                                dataType: 'json',
                                cache: false,
                                success: function (data) {
                                    $.each(data, function (index, item) {
                                        el.append($('<option>').text(item.Text).val(item.Value));
                                    });
                                    if (elVal != null) {
                                        el.val(elVal).change();
                                    }
                                }
                            });

                        }
                    });

                    $('#myModal_Purchase #FeePriceDetailID').change(function () {
                        var value = $(this).val();
                        if (value > 0) {
                            var el = $('#myModal_Purchase #FeePriceDetailID');
                            $.ajax({
                                url: '@Url.Action("FeePriceDetail", "Ajax")',
                                type: 'POST',
                                data: { id: value },
                                cache: false,
                                success: function (data) {
                                    $('#lblFeePriceDetail').html(data);
                                }
                            });

                        }
                    });

                    $('#frmEmployeePoint').submit(function () {
                        $('#myModal_Purchase .show_err').empty();
                        $.ajax({
                            url: '@Url.Action("PurchaseWallet", "Ajax")',
                            type: 'POST',
                            data: $(this).serialize(),
                            dataType: 'json',
                            cache: false,
                            success: function (data) {

                                if (data.r) {
                                    toastr.success(data.m, "Message");
                                    $('#myModal_Purchase').modal('hide');
                                } else {
                                    $('#myModal_Purchase .show_err').html(data.m);
                                    toastr.warning(data.m, "Message");
                                }
                            }
                        });

                        return false;
                    });

                }
            });

        }
    </script>
    @RenderSection("JS", required: false)

</body>
</html>