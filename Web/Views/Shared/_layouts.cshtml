        function feedBack() {
            $('#modalFeedBack').remove();
            $.ajax({
                url: '@Url.Action("FeedBack","Contact")',
                success: function(response) {
                    $('body').append(response);
                    $('#modalFeedBack').modal({
                        keyboard: false,
                        backdrop: 'static',
                        show: true
                    });

                    $('#frmFeedback').submit(function() {
                        var $btn = $('#modalFeedBack button[type="submit"]').button('loading');
                        $.ajax({
                            url: $(this).attr('action'),
                            data: $(this).serialize(),
                            type: 'POST',
                            dataType: 'JSON',
                            success: function(response) {
                                $btn.button('reset');
                                if (response.status) {
                                    $('#modalFeedBack .modal-body').html(response.msg);
                                } else {
                                    toastr.warning(response.msg, "Message");
                                }
                            }
                        });

                        return false;
                    });
                }
            });

            return false;
        }

