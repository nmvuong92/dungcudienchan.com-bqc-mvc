@model VD.Data.Entity.Statistic

<table class="table table-striped table-bordered">
    <tr>
        <td>Hôm nay</td>
        <td>@Model.HomNay</td>
    </tr>
    <tr>
        <td>Hôm qua</td>
        <td>@Model.HomQua</td>
    </tr>

    <tr>
        <td>Tuần này</td>
        <td>@Model.TuanNay</td>
    </tr>
    <tr>
        <td>Tuần trước</td>
        <td>@Model.TuanTruoc</td>
    </tr>
    <tr>
        <td>Tháng này</td>
        <td>@Model.ThangNay</td>
    </tr>
    <tr>
        <td>Tháng trước</td>
        <td>@Model.ThangTruoc</td>

    </tr>
    <tr style="background-color:#fffaf0;">
        <td>Tổng</td>
        <td>@Model.TatCa</td>
    </tr>
</table>